	<modelVersion>4.0.0</modelVersion>
	<groupId>boilerplate-selenium</groupId>
	<artifactId>boilerplate-selenium</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<!-- <parent>
		<groupId>ru.yandex.qatools.allure</groupId>
